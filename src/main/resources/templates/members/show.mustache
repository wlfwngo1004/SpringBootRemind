{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#memberShow}}
        <tr>
            <th>{{id}}</th>
            <td>{{email}}</td>
            <td>{{password}}</td>
        </tr>
    {{/memberShow}}
    </tbody>
</table>
<a href="/members/{{id}}/edit" class="btn btn-primary">수정하기</a>
<a href="/members/{{id}}/delete" class="btn btn-danger">삭제하기</a>
<a href="/members">Go to member list</a>
{{>layouts/footer}}